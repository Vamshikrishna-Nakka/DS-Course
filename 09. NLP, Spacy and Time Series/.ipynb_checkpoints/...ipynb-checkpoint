    "#Iterating around the local tree\n",
    "# Token.lefts and Token.rights attributes provide sequences of syntactic children that occur before and after the token.\n",
    "# There are also two integer-typed attributes, Token.n_lefts and Token.n_rights that give the number of left and right children.\n",
    "\n",
    "doc = nlp(\"bright red apples on the tree\")\n",
    "print([token.text for token in doc[2].lefts])  # ['bright', 'red']\n",
    "print([token.text for token in doc[2].rights])  # ['on']\n",
    "print(doc[2].n_lefts)  # 2\n",
    "print(doc[2].n_rights)  # 1"
   ]
